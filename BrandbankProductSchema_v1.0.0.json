    "itemStructuredNutrition": {
      "type": "object",
      "properties": {
        "groupDefinitions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "name": { "type": "string" },
              "amountHeader": { "type": "string" },
