        },
        "nutrients": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "name": { "type": "string" },
              "unit": {
                "type": "object",
                "properties": {
                  "id": { "type": "integer" },
                  "name": { "type": "string" },
                  "abbreviation": { "type": "string" }
                },
                "required": [ "id", "name", "abbreviation" ]
              },
              "groupValues": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "amount": {
                      "type": [ "object", "null" ],
                      "properties": {
                        "value": { "type": "string" },
                        "qualifierId": { "type": "integer" },
                        "qualifierName": { "type": "string" }
                      }
                    },
                    "referenceIntakeValues": {
                      "type": "array",
                      "items": {
                        "type": [ "object", "null" ],
                        "properties": {
                          "value": { "type": "string" },
                          "qualifierId": { "type": "integer" },
                          "qualifierName": { "type": "string" }
                        }
                      }
                    }
                  },
                  "required": [ "amount", "referenceIntakeValues" ]
                }
              }
            },
            "required": [ "id", "name" ]
          }
        }
      },
      "required": [ "groupDefinitions" ]
    },
