        "rowData": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "nutrient": { "type": "string" },
              "values": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [ "nutrient", "values" ]
          }
        }
      },
      "required": [ "columnHeaders", "rowData" ]
    },
    "perTemplate": {
      "type": "object",
      "properties": {
        "qualifierId": { "type": "integer" },
        "qualifierValue": { "type": "string" },
        "value": { "type": "string" }
      }
    },
    "itemCalculatedNutrition": {
